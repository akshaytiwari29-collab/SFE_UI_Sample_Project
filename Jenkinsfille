node {
    stage('Checkout SCM') {
            git branch: 'main' , url: 'https://github.com/suruchive/SFE_UI_Sample_Project.git' 
    }

    stage('Install Node modules') {
           sh "npm install"
    }

    stage('Build') {
           sh "npm run build"
    }

      stage('Start') {
           sh "npm start"
    }
}